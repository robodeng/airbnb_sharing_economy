    "#Rob's Read file names\n",
    "listings = pd.read_csv('boston_listings.csv')\n",
    "calendar = pd.read_csv('boston_calendar.csv') #NOTE I changed the British for the 'Merican pronunciation\n",
    "reviews = pd.read_csv('boston_reviews.csv')"
   ]
